<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="apple-touch-icon" sizes="114*114" href="<%= require('../icons/apple-touch-icon.png') %>">
    <link rel="icon" type="image/png" sizes="32x32" href="<%= require('../icons/favicon-32x32.png') %>">
    <link rel="icon" type="image/png" sizes="16x16" href="<%= require('../icons/favicon-16x16.png') %>">
    <link rel="manifest" href="<%= require('../icons/manifest.json') %>">
    <link rel="mask-icon" href="<%= require('../icons/safari-pinned-tab.svg') %>" color="#006d56">
    <meta name="theme-color" content="#ffffff">
    <title>
        <%= htmlWebpackPlugin.options.title %>
    </title>
</head>

<body>

    <!-- <div class="loader-container">
        <img class="loader_ellipse bcg" src=<%= require('../images/ellipse.png')%> alt="elipse">
        <img class="loader_rocket bcg" src=<%= require('../images/rocket.png')%> alt="rocket">
    </div> -->
    <!-- Logo -->
    <div class="logo-container">
        <a href="index.html" class="website-name" data-link="true">
            <img src=<%=require( "../images/logo.png") %> alt="Space X Logo">
        </a>
    </div>
    <div class="super-container">
        <div class="button-container">
            <img class="nextButton bcg" src=<%= require('../images/up.png')%> alt="up">
            <img class="previousButton bcg" src=<%= require('../images/down.png')%> alt="up">
        </div>
        <div class="visor-container">
            <div class="first-visor"></div>
            <div class="second-visor"></div>
        </div>
        <!-- Lines used to link visor and informations -->
        <div class="line-container">
            <div class="line line-1"></div>
            <div class="line line-2"></div>
        </div>
        <!-- Informations that will be displayed -->
        <div class="container-info">
            <div class="indications">
                <h1 class="date"></h1>
                <div class="place"></div>
                <div class="shortLine"></div>
                <h2 class="mainInformations">
                <div class="rocket"></div>
                <div class="title">Rocket's name</div>
                <div class="cost"></div>
                <div class="title">Cost of the takeoff test</div>
                </h2>
                <p class="causeOfFailureSmartphone"></p>
                <img class="media bcg" src="" alt="rocket takeoff" />
                <h3 class="details">Details</h3>
                <p class="textDetails">
                </p>
                <h3 class="consequences">Consequences</h3>
                <p class="textConsequences">
                </p>
            </div>
            <p class="causeOfFailure"></p>
        </div>
        <!-- 3D Container -->
        <div class="container">
            <img data-parallax ="true" class="plan index-1 bcg" src=<%= require('../images/points.png')%>  alt="Test">
            <img data-parallax ="true" class="plan index-2 bcg" src=<%= require('../images/star-plans-index.png')%>  alt="Test">
            <img data-parallax ="true" class="plan index-3 bcg" src=<%= require('../images/star-plans-index.png')%>  alt="Test">
            <img data-parallax ="true" class="plan index-4 bcg" src=<%= require('../images/star-plans-index.png')%>  alt="Test">
            <img data-parallax ="true" class="plan index-5 bcg" src=<%= require('../images/star-plans-index.png')%>  alt="Test">               
        </div>
    </div>
</body>

</html>